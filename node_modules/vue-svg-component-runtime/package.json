{
  "name": "vue-svg-component-runtime",
  "version": "1.0.1",
  "description": "Runtime for supporting SVG-as-Vue Components",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "typings": "lib-esm/index.d.ts",
  "keywords": [
    "vue",
    "vuejs",
    "svg",
    "svg-icons"
  ],
  "repository": "https://github.com/wadetandy/vue-svg-component-builder",
  "author": "Wade Tandy <wade.tandy@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist ./build ./coverage ./lib ./lib-esm",
    "prebuild": "yarn run clean && yarn test",
    "build": "npm-run-all build:node build:es6",
    "build:node": "yarn run tsc -p ./tsconfig.node.json",
    "build:es6": "yarn run tsc -p ./tsconfig.lib.json",
    "test": "NODE_ENV=test mocha --opts test/mocha.opts",
    "test:watch": "yarn test --watch",
    "prepublish": "yarn build"
  },
  "peerDependencies": {
    "vue": ">= 2.5.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.3",
    "@types/mocha": "5.2.0",
    "@types/node": "7.0.63",
    "@types/sinon": "4.3.1",
    "chai": "4.1.2",
    "jsdom": "11.10.0",
    "jsdom-global": "3.0.2",
    "mocha": "5.1.1",
    "npm-run-all": "4.1.2",
    "rimraf": "2.6.2",
    "sinon": "5.0.3",
    "ts-node": "6.0.2",
    "typescript": "2.8.3",
    "vue": "2.5.16",
    "vue-template-compiler": "2.5.16",
    "vue-test-utils": "1.0.0-beta.11"
  }
}
